<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.aluswallet">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/complete"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        tools:targetApi="31">
        <activity
            android:name=".PaymentHistory"
            android:exported="false" />
        <activity
            android:name=".DetailsOfPayment"
            android:exported="false" />
        <activity
            android:name=".ViewAllPaymentRequest"
            android:exported="false" />
        <activity
            android:name=".FeeSummary"
            android:exported="false" />
        <activity
            android:name=".AlusAdmissionFee"
            android:exported="false" />
        <activity
            android:name=".RemainingKycStepOne"
            android:exported="false" />
        <activity
            android:name=".RemainingKyc"
            android:exported="false" />
        <activity
            android:name=".KvcSecondeStep"
            android:exported="false" />
        <activity
            android:name=".CompleteYourKyc"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:parentActivityName=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>